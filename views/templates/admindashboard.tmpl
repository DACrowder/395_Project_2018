{{define "admindashboard"}}

{{template "adminheader"}}

<main>
    <div id="familyList">
        <table>
            <tr>
                <th>ID</th>
                <th>Family</th>
                <th>Scheduled this Week</th>
            </tr>
                {{range .}}
                <tr>
                    <td>{{.FamilyID}}</td>
                    <td>{{.FamilyName}}</td>
                    <td>{{.WeekHours}}</td>
                </tr>
                {{end}}
            </tr>
        </table>
    </div>
</main>

{{template "footer"}}

{{end}}