{{define "admindashboard"}}

{{template "adminheader" .}}

<script id="tmpl_familyList" type="x-dot-template">
    <table>
        <tr>
            <th>ID</th>
            <th>Family</th>
            <th>Scheduled this Week</th>
        </tr>
        {{`{{~it :value:index}}`}}
        <tr>
            <td>{{`{{=value.familyId}}`}}</td>
            <td>{{`{{=value.familyName}}`}}</td>
            <td>{{`{{=value.weekHours}}`}}</td>
        </tr>
        {{`{{~}}`}}
    </table>
</script>

<main>
    <div class="container">
        <div id="displayData"></div>
    </div>
</main>

{{template "footer"}}

{{end}}